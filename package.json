{
  "name": "word-trainer-plus",
  "version": "1.0.0",
  "description": "Demonstration of PassportJS authentication in React w/ Redux.",
  "main": "devStart.js",
  "scripts": {
    "build:server": "babel src/server --out-dir lib/server",
    "build:ui": "webpack --progress",
    "build": "npm run clean && npm run build:server && npm run build:ui",
    "heroku-postbuild": "npm run build",
    "clean": "rimraf lib public/assets",
    "server": "better-npm-run server",
    "server:dev": "better-npm-run server:dev",
    "start": "npm run server",
    "start:dev": "npm run server:dev"
  },
  "betterScripts": {
    "server": {
      "command": "node lib/server/index.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "server:dev": {
      "command": "nodemon --exec babel-node src/server/index.js --ignore src/client --mode development",
      "env": {
        "NODE_ENV": "development"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/golonikum/word-trainer-plus.git"
  },
  "dependencies": {
    "@babel/cli": "7.4.3",
    "@babel/core": "7.4.3",
    "@babel/node": "7.2.2",
    "@babel/plugin-proposal-class-properties": "7.4.0",
    "@babel/preset-env": "7.4.3",
    "@babel/preset-react": "7.0.0",
    "axios": "0.18.0",
    "babel-loader": "8.0.5",
    "bcrypt-nodejs": "0.0.3",
    "better-npm-run": "0.0.8",
    "bluebird": "3.5.3",
    "body-parser": "1.18.3",
    "bootstrap": "4.3.1",
    "connect-mongo": "1.1.0",
    "connected-react-router": "6.3.2",
    "crypto": "1.0.1",
    "detect-node": "2.0.4",
    "express": "4.16.4",
    "express-session": "1.15.6",
    "history": "4.9.0",
    "html-webpack-plugin": "3.2.0",
    "mongoose": "5.4.21",
    "nodemon": "1.18.10",
    "passport": "0.4.0",
    "passport-local": "1.0.0",
    "react": "16.8.6",
    "react-bootstrap": "1.0.0-beta.6",
    "react-dom": "16.8.6",
    "react-redux": "6.0.1",
    "react-router-dom": "5.0.0",
    "redux": "4.0.1",
    "redux-logger": "3.0.6",
    "redux-thunk": "2.3.0",
    "rimraf": "2.5.2",
    "terser-webpack-plugin": "1.2.3",
    "uglifyjs-webpack-plugin": "2.1.2",
    "webpack": "4.29.6",
    "webpack-dev-server": "3.2.1",
    "webpack-hot-middleware": "2.24.3",
    "webpack-livereload-plugin": "2.2.0",
    "webpack-cli": "3.3.0",
    "webpack-dev-middleware": "3.6.2"
  },
  "author": "Nikita Golov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/golonikum/word-trainer-plus/issues"
  },
  "homepage": "https://github.com/golonikum/word-trainer-plus#readme"
}
